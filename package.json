{
  "name": "ambrosus-staking",
  "title": "Ambrosus Arcadia Staking",
  "description": "Get AMB Rewards. No node needed.",
  "private": true,
  "keywords": [],
  "homepage": "https://staking.ambrosus.io/",
  "license": "GPL-3.0-only",
  "main": "src/index.js",
  "version": "1.0.1",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts --max_old_space_size=4096 build ",
    "serve": "react-scripts build && serve -s build",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "lint": "eslint ./ --ignore-path .gitignore",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write \"{,!(node_modules)/**/}*.js\"",
    "stylelint": "stylelint src/**/*.scss",
    "stylelint:fix": "stylelint src/**/*.scss --fix"
  },
  "dependencies": {
    "@inc4/eslint-config-react": "1.0.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-gtm-module": "^2.0.11",
    "react-loading": "^2.0.3",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "react-svg": "^14.0.10",
    "sass": "^1.35.2",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "dotenv": "^10.0.0",
    "eslint": "^8.24.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "prettier": "^2.3.2",
    "react-error-overlay": "^6.0.9",
    "source-map-explorer": "^2.5.2",
    "stylelint": "^13.13.1",
    "stylelint-config-idiomatic-order": "^8.1.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.4.0",
    "stylelint-order": "^4.1.0",
    "stylelint-order-config-standard": "^0.1.3",
    "stylelint-prettier": "^1.2.0",
    "stylelint-scss": "^3.20.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": "12 || 14 || 15 || 16 ",
    "npm": ">=6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ambrosus/staking-frontend.git"
  },
  "bugs": {
    "url": "https://github.com/ambrosus/staking-frontend/issues"
  }
}
